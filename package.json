{
  "name": "cheat-day-cheesecakes",
  "author": "Garrett Weems <gwgraphicdesign@gmail.com> (https://glweems.com)",
  "scripts": {
    "build": "gatbsy clean && gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "prettier --ignore-path .gitignore --write \"**/*.{ts,tsx,js,json,yml,md}\"",
    "lint:es": "eslint --ignore-path .gitignore \"**/*.{ts,tsx,js}\"",
    "lint:fix": "npm run format && npm run lint",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx,js}\"",
    "lint": "npm run lint:es && npm run lint:style",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "type-check": "tsc --noEmit"
  },
  "description": "A Gatsby starter with strict linting and auto-formatting rules.",
  "version": "1.0.0",
  "repository": "github:glweems/gatsby-starter-ultra-modern",
  "keywords": [
    "gatsby-starter",
    "gatsby",
    "redux",
    "starter",
    "styled-components",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn build"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/glweems/gatsby-starter-ultra-modern/issues"
  },
  "homepage": "https://github.com/glweems/gatsby-starter-ultra-modern#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@microlink/react": "^4.0.1",
    "@rhysforyou/gatsby-plugin-react-helmet-async": "^0.1.1",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.6.2",
    "@types/react-burger-menu": "^2.2.5",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/parser": "^1.11.0",
    "axios": "^0.19.0",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv": "^8.0.0",
    "gatsby": "^2.13.21",
    "gatsby-cli": "^2.7.14",
    "gatsby-image": "^2.2.6",
    "gatsby-plugin-config": "^1.0.7",
    "gatsby-plugin-layout": "^1.1.2",
    "gatsby-plugin-manifest": "^2.2.1",
    "gatsby-plugin-sass": "^2.1.3",
    "gatsby-plugin-sharp": "^2.2.8",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-plugin-typescript": "^2.1.0",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-source-filesystem": "^2.1.5",
    "gatsby-transformer-sharp": "^2.2.4",
    "google-maps-react": "^2.0.2",
    "node-sass": "^4.12.0",
    "react": "^16.8.6",
    "react-burger-menu": "^2.6.10",
    "react-dom": "^16.8.6",
    "react-helmet-async": "^1.0.2",
    "react-parallax": "^2.2.0",
    "react-social-icons": "^4.1.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "styled-media-query": "^2.1.2",
    "ts-graphql": "^6.0.0",
    "typography": "^0.16.19",
    "typography-theme-funston": "^0.16.19"
  },
  "devDependencies": {
    "@divyenduz/ts-graphql-plugin": "^0.1.0",
    "@types/react": "^16.8.23",
    "@types/react-helmet-async": "^1.0.0",
    "@types/styled-components": "^4.1.18",
    "@types/typography": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.0.0",
    "stylelint-processor-styled-components": "^1.8.0",
    "typescript": "^3.5.3"
  }
}
